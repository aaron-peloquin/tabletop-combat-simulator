{
  "name": "tabletop-combat-simulator",
  "version": "0.0.1",
  "description": "A simple tabletop game combat simulator",
  "main": "server.js",
  "repository": "git@github.com:aaron-peloquin/tabletop-combat-simulator.git",
  "author": "Aaron <aaron4uk@gmail.com>",
  "license": "MIT",
  "quokka": {
    "babel": true,
    "plugins": [
      "jsdom-quokka-plugin"
    ]
  },
  "scripts": {
    "build": "next build ./src/",
    "cypressTest": "cypress open --config viewportWidth=1440,viewportHeight=1900",
    "cypressRecord": "cypress run --record --config viewportWidth=1440,viewportHeight=1900",
    "dev": "node ./src/devServer.js",
    "docker:build": "docker build -t aaronpeloquin/tabletop-combat-simulator .",
    "docker:run": "docker run -p 80:80 aaronpeloquin/tabletop-combat-simulator:latest",
    "docker:push": "docker push aaronpeloquin/tabletop-combat-simulator:latest",
    "start": "node ./src/prodServer.js",
    "test": "jest --watch"
  },
  "dependencies": {
    "@material-ui/core": "^3.3.0",
    "@material-ui/icons": "^3.0.1",
    "enzyme-redux": "^0.2.1",
    "express": "^4.16.4",
    "immutable": "^4.0.0-rc.12",
    "isomorphic-fetch": "^2.2.1",
    "next": "^7.0.2",
    "next-redux-wrapper": "^2.0.0",
    "react": "^16.8.3",
    "react-dom": "^16.5.2",
    "react-jss": "^8.6.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
    "seedrandom": "^2.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@percy/cypress": "^1.0.6",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cypress": "^3.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.8.0",
    "eslint-config-google": "^0.11.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "jsdom-quokka-plugin": "^1.0.13",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.8.3",
    "redux-test-utils": "^0.3.0"
  }
}
